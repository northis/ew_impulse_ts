  </PropertyGroup>
  <ItemGroup>
    <Compile Remove="EasyGartleyIndicatorBase.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="createlinks.cmd" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="cTrader.Automate" Version="1.0.6" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="EasyGartleyIndicatorLib.csproj">
      <Private>True</Private>
    </ProjectReference>
  </ItemGroup>
</Project>
